<div class="container">
    <h2>Administración de Usuarios</h2>
    <table>
        <tr>
            <th>Usuario</th>
            <th>Email</th>
            <th>Rol</th>
            <th>Fecha de Creación</th>
            <th>Acciones</th>
        </tr>
        {{#each usuarios}} <!-- Iterar sobre la lista de usuarios pasada desde el servidor -->
        <tr>
            <td>{{this.usuario}}</td> <!-- Mostrar el nombre del usuario -->
            <td>{{this.email}}</td> <!-- Mostrar el email del usuario -->
            <td>{{this.rol}}</td> <!-- Mostrar el rol del usuario -->
            <td>{{this.fecha_creacion}}</td> <!-- Mostrar la fecha de creación -->
            <td>
                <button class="btn">Editar</button> <!-- Botón para editar -->
                <button class="btn">Borrar</button> <!-- Botón para borrar -->
            </td>
        </tr>
        {{else}}
        <tr>
            <td colspan="5">No hay usuarios disponibles :c</td> <!-- Mensaje si no hay usuarios -->
        </tr>
        {{/each}}
    </table>
</div>
