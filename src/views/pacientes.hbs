<div class="container">
    <h2>Pacientes</h2>
    <table>
        <tr>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Tel√©fono</th>
            <th>Email</th>
            <th>Fecha de Registro</th>
            <th>Editar paciente</th>
        </tr>
        {{#each pacientes}} <!-- itera sobre los pacientes pasados desde la ruta -->
        <tr>
            <td>{{nombre}}</td> <!-- -->
            <td>{{apellido}}</td> 
            <td>{{telefono}}</td> 
            <td>{{email}}</td> 
            <td>{{fecha_registro}}</td> 
            <td>
                <button class="btn">Editar</button> 
                <button class="btn">Borrar</button>
            </td>
        </tr>
        {{/each}} <!-- Cierra el bucle x cada paciente -->
    </table>
</div>
